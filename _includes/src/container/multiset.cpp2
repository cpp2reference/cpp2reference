main: () = {
    c: std::multiset = (1, 2, 3, 4, 1, 2, 3, 4);
 
    print:= :() = {
        std::cout << "c = { ";
        for c&$* do (n) {
            std::cout << "(n)$ ";
        }
        std::cout << "}\n";
    };
    print();
 
    std::cout << "Erase all odd numbers:\n";
    (copy it:= c.begin())
    while it != c.end() {
        if it* % 2 != 0 {
            it = c.erase(it);
        }
        else {
            it++;
        }
    }
    print();
 
    std::cout << "Erase 1, erased count: (c.erase(1))$\n";
    std::cout << "Erase 2, erased count: (c.erase(2))$\n";
    std::cout << "Erase 2, erased count: (c.erase(2))$\n";
    print();
}